<p id="notice"><%= notice %></p>

<h1>Typepayments</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Descricao</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typepayments.each do |typepayment| %>
      <tr>
        <td><%= typepayment.nome %></td>
        <td><%= typepayment.descricao %></td>
        <td><%= typepayment.user %></td>
        <td><%= link_to 'Show', typepayment %></td>
        <td><%= link_to 'Edit', edit_typepayment_path(typepayment) %></td>
        <td><%= link_to 'Destroy', typepayment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Typepayment', new_typepayment_path %>
